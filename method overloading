import java.io.*;
class m1
{
void sum()
{
System.out.println("method overloading");
}
int sum(int x)
{
return x*x;
}
int sum(int a,int b)
{
return a+b;
}
float sum(float m,float n)
{
return m-n;
}
float sum(float z)
{
return z+z;
}
float sum(int c,float d)
{
return c*d;
}
}
class method
{
public static void main(String args[])
{ 
m1 s=new m1();
s.sum();
System.out.println(s.sum(10));
System.out.println(s.sum(5,6));
System.out.println(s.sum(3.2f,4.5f));
System.out.println(s.sum(4.7f));
}
}
